/* pages/lipeiInfo/index.wxss */
page{
  width: 100%;
  height: 100%;
}
.AnLiImg{
  width: 100%;
  height: 400rpx;
}
.AnLiText{
  width: 100%;
  height: 400rpx;
  text-indent: 24rpx;
  font-size: 26rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.tishi{
  padding-bottom: 20rpx;
  border-bottom: 1rpx dashed #1AAD16;
}
form>view{
  color: #1AAD16;
}
.userName, .userPhone{
  margin-top: 20rpx;
}
input{
  border-bottom: 1px solid #1AAD16;
  margin: 20rpx 0;
  box-sizing: border-box;
  padding-left: 20rpx;
  height: 60rpx;
}
button{
  margin-bottom: 20rpx;
}