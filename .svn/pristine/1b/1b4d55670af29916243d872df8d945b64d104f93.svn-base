/* pages/my/index.wxss */

page {
  padding: 0rpx 20rpx;
}

.btns {
  top: 40rpx;
  left: 0rpx;
  opacity: 0;
  width: 100%;
  height: 120rpx;
  position: absolute;
}
.renzhen{
  position: absolute;
  left: 0rpx;
  top: -4rpx;
  width: 100%;
  height: 100%;
  opacity: 0;
  z-index: 9999999999;
}
.logining {
  height: 80rpx;
  line-height: 50rpx;
  font-size: 32rpx;
  position: relative;
  margin: 40rpx 0rpx;
}

.logining::after {
  display: block;
  content: '';
  height: 40rpx;
  background: #f3f3f3;
  position: relative;
  left: -18rpx;
  margin: 20rpx 0rpx;
}

.logining>image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background: #f3f3f3;
  vertical-align: middle;
  box-sizing: border-box;
  padding: 20rpx;
}

.logining>image:last-child {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  background: #fff;
  right: 80rpx;
  top: 38rpx;
}

.status {
  float: right;
  margin-right: 90rpx;
  font-size: 28rpx;
  margin-top: 14rpx;
}

.status2 {
  /* float: right;
  margin-right: 90rpx; */
  margin-left: 40rpx;
}

.ueserInfo {
  position: absolute;
  width: 56%;
  height: 80rpx;
  left: 120rpx;
  top: -12rpx;
}

.ueserInfo>text:last-child {
  display: inline-block;
  height: 100%;
  width: 100%;
  position: absolute;
  left: 0rpx;
  top: 20rpx;
  line-height: 75rpx;
}

.xX {
  margin-bottom: 14rpx;
  position: relative;
  top: 40rpx;
  height: 100rpx;
}

.xX>image {
  width: 60rpx;
  height: 60rpx;
  /* border-radius: 30rpx; */
  line-height: 60rpx;
  margin-left: 10rpx;
  vertical-align: middle;
  margin-top: 10rpx;
}

.xX>text:nth-child(2), .xX>view:nth-child(2) {
  display: inline-block;
  width: calc(100% - 100rpx);
  height: 80rpx;
  font-size: 32rpx;
  position: absolute;
  left: 100rpx;
  top: 20rpx;
  border-bottom: 1rpx solid #f3f3f3;
}

.bindZhi {
  width: 80%;
  height: 100%;
  display: inline-block;
}

.bingzhiT {
  display: inline-block;
  width: 18%;
  height: 100%;
  float: right;
  text-align: center;
  line-height: 80rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
}

.xX>image:last-child {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 80rpx;
  top: 28rpx;
  margin: 0rpx;
  padding: 0rpx;
}

.zT {
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  border-radius: 10rpx;
  position: absolute;
  right: 120rpx;
  top: 34rpx;
  background: red;
}

