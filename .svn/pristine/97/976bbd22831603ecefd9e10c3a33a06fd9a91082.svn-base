}
.isSearch{
  display: inline-block;
  width: 10%;
  height: calc(100% - 6rpx);
  text-align: center;
  line-height: 80rpx;
  font-size: 24rpx;
  border: 1rpx solid #ccc;
  border-left: none;
  float: left;
  margin-top: 2rpx;
}
.tab-h {
  height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  background: #fff;
  font-size: 28rpx;
  white-space: nowrap;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
}

.tab-item {
  width: 20%;
  /* margin: 0 36rpx; */
  display: inline-block;
  text-align: center;
}

.tab-item.active {
  color: #467de4;
  position: relative;
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 8rpx;
  width: 100%;
  background: #467de4;
  position: absolute;
  bottom: 0;
  left: 5rpx;
  border-radius: 16rpx;
}

.item-ans {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  background: #f5f5f9;
}
.item-ans>image{
  width: 300rpx;
  height: 460rpx;
  margin: 30rpx 0rpx;
}
.item-ans>text{
  width: 80%;
  display: inline-block;
}
.item-ans>text:first-child{
  margin-top: 20rpx;
}
.item-ans>text:last-child{
  margin-bottom: 20rpx;
}
.avatar {
  width: 100rpx;
  height: 100rpx;
  position: relative;
  padding-right: 30rpx;
}

.avatar .img {
  width: 100%;
  height: 100%;
}

.avatar .doyen {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  bottom: -2px;
  right: 20rpx;
}

.expertInfo {
  font-size: 12px;
  flex-grow: 2;
  color: #b0b0b0;
  line-height: 1.5em;
}

.expertInfo .name {
  font-size: 16px;
  color: #000;
  margin-bottom: 6px;
}

.askBtn {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 14px;
  border-radius: 60rpx;
  border: 1px solid #467de4;
  color: #467de4;
}

.tab-content {
  margin-top: 80rpx;
  height: 676rpx;
