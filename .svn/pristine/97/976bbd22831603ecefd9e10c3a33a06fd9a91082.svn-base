}
.leftAdress{
  width: 20%;
  height: calc(100% + 0rpx);
  display: inline-block;
  font-size: 24rpx;
  border: 1rpx solid #ccc;
  border-right: none;
  box-sizing: border-box;
  /* padding-left: 16rpx; */
  float: left;
  margin-top: 2rpx;
}
.section{
  display: inline-block;
}
.picker1{
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  width: 20%;
