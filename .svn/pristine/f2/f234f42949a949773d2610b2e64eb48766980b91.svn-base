/* pages/my/index.wxss */
page {
  /* padding: 0rpx 20rpx; */
}

.btns {
  top: 200rpx;
  left: calc(50% - 150rpx);
  opacity: 0;
  width: 300rpx;
  height: 80rpx;
  position: absolute;
}
.renzhen{
  position: absolute;
  left: 0rpx;
  top: -4rpx;
  width: 100%;
  height: 100%;
  opacity: 0;
  z-index: 9999999999;
}
.logining {
  height: 320rpx;
  line-height: 50rpx;
  font-size: 32rpx;
  position: relative;
  background: #ec7c03;
  /* margin: 40rpx 0rpx; */
}

