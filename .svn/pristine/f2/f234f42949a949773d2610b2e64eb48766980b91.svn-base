.logining>image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
  background: #f3f3f3;
  vertical-align: middle;
  box-sizing: border-box;
  padding: 20rpx;
  margin: 0 auto;
  position: absolute;
  top: 50rpx;
  left: calc(50% - 60rpx);
}

.logining>image:last-child {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  background: #fff;
  right: 178rpx;
  top: 38rpx;
}

.status {
  float: right;
  margin-right: 90rpx;
  font-size: 28rpx;
  margin-top: 14rpx;
}

.status2 {
  top: 200rpx;
  left: calc(50% - 150rpx);
  width: 300rpx;
  height: 80rpx;
  position: absolute;
  text-align: center;
  line-height: 80rpx;
  border-radius: 50rpx;
  background: #fff;
  color: #ec7c03;
}

.ueserInfo {
  position: absolute;
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  left: calc(50% - 150rpx);
  bottom: 30rpx;
}

.ueserInfo>text:last-child {
  display: inline-block;
  height: 100%;
  width: 100%;
  background: #fff;
  border-radius: 50rpx;
  color: #ec7c03;
}

.business{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
  position: relative;
}

.xX {
  margin-bottom: 14rpx;
  position: relative;
  top: 20rpx;
  margin-top: 20rpx;
  height: 130rpx;
  width: 22%
}

.xX>image {
  width: 70rpx;
  height: 70rpx;
  /* border-radius: 30rpx; */
  line-height: 70rpx;
  margin-left: 10rpx;
  vertical-align: middle;
  margin-top: 10rpx;
  margin: 0 50rpx;
  display: block;
}

.xX>text:nth-child(2), .xX>view:nth-child(2) {
  display: block;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  width: 100%;
  font-size: 26rpx;
}

.bindZhi {
  width: 80%;
  height: 100%;
  display: inline-block;
}

.bingzhiT {
  display: inline-block;
  width: 18%;
  height: 100%;
  float: right;
  text-align: center;
  line-height: 80rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
}

.xX>image:last-child {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 80rpx;
  top: 28rpx;
  margin: 0rpx;
  padding: 0rpx;
}

.zT {
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  border-radius: 10rpx;
  position: absolute;
  right: 120rpx;
  top: 34rpx;
  background: red;
}

.btn{
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  left: 16rpx;
  bottom: 2rpx;
  opacity: 0;
}
